{
  "name": "ter-ui",
  "version": "0.1.0",
  "private": false,
  "main": "lib/index.js",
  "module": "lib/index.js",
  "description": "A React component library for learning",
  "author": "hanting",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "homepage": "https://github.com/zhtzhtx/TerUI",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhtzhtx/TerUI"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
